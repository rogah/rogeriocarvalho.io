{
  "name": "rogeriocarvalho.io",
  "version": "0.0.1",
  "description": "Rogerio Carvalho",
  "keywords": [
    "Rogerio",
    "Wenzel",
    "Carvalho",
    "Rogerio Carvalho"
  ],
  "author": "Rogerio Carvalho",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/rogah/rogeriocarvalho.io.git"
  },
  "scripts": {
    "env": "env",
    "clean": "rimraf build && mkdirp build",
    "copy": "cp -R src/ build/",
    "lint": "jshint src/*.js",
    "beautify:js": "js-beautify **/*.js -r",
    "deploy": "npm run clean -s && npm run copy",
    "build": "npm run lint && npm run beautify:js",
    "postbuild": "npm run deploy",
    "prestart": "npm run build",
    "start": "iojs build/app.js",
    "test": "cucumber.js test/features -r test/features/step_definitions -r test/features/support -f pretty"
  },
  "dependencies": {
    "express": "^4.12.4",
    "js-beautify": "^1.5.5",
    "jshint": "^2.7.0",
    "jshint-stylish": "^1.0.0",
    "mkdirp": "^0.5.1",
    "nodemon": "^1.3.7",
    "rimraf": "^2.3.4"
  },
  "devDependencies": {
    "cucumber": "^0.4.9",
    "mocha": "^2.2.5",
    "should": "^6.0.3",
    "zombie": "^4.0.10"
  },
  "engines": {
    "iojs": "2.0.x",
    "npm": "2.10.x"
  }
}
